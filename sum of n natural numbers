#include <iostream>
using namespace std;

int main() {
//  problem : sum of n natural number 

    cout << "Me tumse starting aur ending number puchunga, jiske beech ka sum chahiye." << endl;

    int sum = 0, i, n, y;

    cout << "Starting number daalo: "<<endl;
   

    cout << "Ending number daalo: "<<endl;
    cin >>y>>n;

    for (i = n; i <= y; i++) {
        sum += i;
    }

    cout << "The sum from " << n << " to " << y << " is: " << sum << endl;

    return 0;
}
