#include<iostream>
using namespace std;
int main(){
 
   // print weeks name according to thier numbers
   int a;
   cout<< "enter a numer between 1 to 7:  "<<endl;
   for(int i=0;i<10;i++){
   cin>>a;
   if(a==1){
    cout<<"monday"<<endl;
   }
   else if (a==2){
    cout<<"tuesday"<<endl;
   }

 else if (a==3){
    cout<<"wednesday"<<endl;
   }
 else if (a==4){
    cout<<"thursday"<<endl;
   }
 else if (a==5){
    cout<<"friday"<<endl;
   }
 else if (a==6){
    cout<<"saturday"<<endl;
   }

 else if (a==7){
    cout<<"sunday"<<endl;
   }
   else{
    cout<<"bhai shi number dal le vrna ....."<<endl;
   }
}
    }
