#include<iostream>
using namespace std;
int main(){
   // problem: print factorial no..
   for(int j=1; j<=10;j=j+1){
   int n;
   cout<<" enter an positive number jiska factorial nikalna hai: ";
   cin >> n;
   int fact = 1;
   for (int i=1; i<=n; i=i+1){
           fact = fact * i;
   }
   cout<< fact << endl;
    }
   return 0;
}

